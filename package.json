    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.4.0",
    "immutable": "^3.8.1",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.2",
    "underscore": "^1.8.3"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1"
