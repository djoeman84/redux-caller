{
  "name": "redux-caller",
  "version": "1.2.0",
  "license": "MIT",
  "description": "Redux Calls you: Simple Library to have redux reducers call a method on your store",
  "keywords": [
    "redux",
    "immutable",
    "immutablejs",
    "record",
    "caller"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/djoeman84/redux-caller.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src -d dist",
